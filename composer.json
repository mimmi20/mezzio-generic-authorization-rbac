{
  "name": "mimmi20/mezzio-generic-authorization-rbac",
  "type": "library",
  "description": "Provides a GitHub repository mezzio-generic-authorization-rbac for a PHP library, using GitHub actions.",
  "homepage": "https://github.com/mimmi20/mezzio-generic-authorization-rbac",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "laminas/laminas-permissions-rbac": "^2.5.1 || ^3.0.0",
    "mimmi20/mezzio-generic-authorization": "dev-master",
    "psr/container": "^1.0.0",
    "psr/http-message": "^1.0.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "~2.8.0",
    "infection/infection": "~0.13.6 || ~0.18",
    "mimmi20/coding-standard": "~1.0.4",
    "pepakriz/phpstan-exception-rules": "~0.11.1",
    "phpstan/extension-installer": "~1.0.5",
    "phpstan/phpstan": "~0.12.50",
    "phpstan/phpstan-beberlei-assert": "~0.12.3",
    "phpstan/phpstan-deprecation-rules": "~0.12.5",
    "phpstan/phpstan-phpunit": "~0.12.16",
    "phpunit/phpunit": "~7.5.20 || ~8.5.8 || ~9.4.2",
    "laminas/laminas-servicemanager": "^3.4.1"
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0",
    "phpspec/prophecy": "<1.10.2"
  },
  "autoload": {
    "psr-4": {
      "Mezzio\\GenericAuthorization\\Rbac\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzioTest\\GenericAuthorization\\Rbac\\": "test/"
    }
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-generic-authorization-rbac/issues",
    "source": "https://github.com/mimmi20/mezzio-generic-authorization-rbac"
  },
  "extra": {
    "laminas": {
      "config-provider": "Mezzio\\GenericAuthorization\\Rbac\\ConfigProvider"
    }
  }
}
