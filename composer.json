{
  "name": "mimmi20/mezzio-generic-authorization-rbac",
  "type": "library",
  "description": "Provides a laminas-permissions-rbac adapter for mezzio-generic-authorization.",
  "keywords": [
    "mezzio",
    "authorization",
    "middleware",
    "psr-7",
    "psr-15",
    "rbac"
  ],
  "homepage": "https://github.com/mimmi20/mezzio-generic-authorization-rbac",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "laminas/laminas-permissions-rbac": "~3.0.2",
    "mimmi20/mezzio-generic-authorization": "dev-master",
    "psr/container": "^1.0.0",
    "psr/http-message": "^1.0.1"
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0",
    "mezzio/mezzio-authorization-rbac": "*"
  },
  "require-dev": {
    "infection/infection": "~0.13.6 || ~0.18 || ~0.20.2",
    "mimmi20/coding-standard": "~1.0.7",
    "pepakriz/phpstan-exception-rules": "~0.11.2",
    "phpstan/extension-installer": "~1.1.0",
    "phpstan/phpstan": "~0.12.63",
    "phpstan/phpstan-beberlei-assert": "~0.12.4",
    "phpstan/phpstan-deprecation-rules": "~0.12.6",
    "phpstan/phpstan-phpunit": "~0.12.17",
    "phpstan/phpstan-strict-rules": "^0.12.7",
    "phpunit/phpunit": "~7.5.20 || ~8.5.11 || ~9.4.3"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mezzio\\GenericAuthorization\\Rbac\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mezzio\\GenericAuthorization\\Rbac\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzioTest\\GenericAuthorization\\Rbac\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-generic-authorization-rbac/issues",
    "source": "https://github.com/mimmi20/mezzio-generic-authorization-rbac"
  }
}
