{
  "name": "mimmi20/mezzio-generic-authorization-rbac",
  "description": "Provides a laminas-permissions-rbac adapter for mezzio-generic-authorization.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "mezzio",
    "authorization",
    "middleware",
    "psr-7",
    "psr-15",
    "rbac"
  ],
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/mezzio-generic-authorization-rbac",
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-generic-authorization-rbac/issues",
    "source": "https://github.com/mimmi20/mezzio-generic-authorization-rbac"
  },
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "ext-mbstring": "*",
    "laminas/laminas-permissions-rbac": "^3.2.0",
    "mimmi20/mezzio-generic-authorization": "^2.0.5",
    "psr/container": "^1.1.2 || ^2.0.2",
    "psr/http-message": "^1.0.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.18.0",
    "laminas/laminas-dependency-plugin": "^2.2.0",
    "laminas/laminas-servicemanager": "^3.10.0",
    "mimmi20/coding-standard": "^2.5.2",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^1.10.21",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-phpunit": "^1.0.0",
    "phpunit/phpunit": "^9.5.10",
    "rector/rector": "^0.12.8"
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0",
    "mezzio/mezzio-authorization-rbac": "*"
  },
  "suggest": {
    "laminas/laminas-servicemanager": "to use the factories"
  },
  "autoload": {
    "psr-4": {
      "Mezzio\\GenericAuthorization\\Rbac\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzioTest\\GenericAuthorization\\Rbac\\": "test/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mezzio\\GenericAuthorization\\Rbac\\ConfigProvider"
    }
  }
}
